{
  "name": "seespee",
  "version": "2.1.1",
  "description": "Create a Content-Security-Policy for a website based on the statically decidable relations",
  "main": "lib/seespee.js",
  "scripts": {
    "test": "mocha",
    "lint": "eslint .",
    "ci": "npm test && npm run lint && npm run coverage",
    "coverage": "nyc --reporter=lcov --reporter=text --all -- mocha && echo google-chrome coverage/lcov-report/index.html"
  },
  "repository": {
    "type": "git",
    "url": "git://github.com/papandreou/seespee/"
  },
  "keywords": [
    "CSP",
    "Content-Security-Policy",
    "XSS"
  ],
  "author": "Andreas Lind <andreaslindpetersen@gmail.com>",
  "license": "BSD-3-Clause",
  "dependencies": {
    "assetgraph": "^6.0.0",
    "chalk": "^3.0.0",
    "lodash": "^4.16.4",
    "magicpen": "^6.0.2",
    "magicpen-prism": "^4.0.0",
    "urltools": "^0.2.1",
    "yargs": "^15.0.1"
  },
  "bin": {
    "seespee": "lib/cli.js"
  },
  "devDependencies": {
    "coveralls": "^3.0.1",
    "eslint": "^6.0.0",
    "eslint-config-prettier": "^6.0.0",
    "eslint-config-standard": "^14.0.0",
    "eslint-plugin-import": "^2.18.2",
    "eslint-plugin-mocha": "^6.0.0",
    "eslint-plugin-node": "^11.0.0",
    "eslint-plugin-promise": "^4.2.1",
    "eslint-plugin-standard": "^4.0.0",
    "httpception": "^3.0.0",
    "mocha": "^6.0.0",
    "nyc": "^15.0.0",
    "prettier": "~1.19.1",
    "unexpected": "^11.0.0"
  },
  "nyc": {
    "include": [
      "lib/**"
    ]
  }
